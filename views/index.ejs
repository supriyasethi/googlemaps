<html>
  <head>
    <title>SimpleMap</title>
    <script src="./app.js"></script>
    <script
      src="https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyDKTBaI9thYqqGA6ICXE3PzOBcYYRNhVM8&sensor=true"
      ;
      async
      defer
    ></script>

    <style>
      * {
        margin: 0;
        padding: 0;
      }
      #map {
        height: 50vh;
        width: 100%;
      }
      .contained {
        height: 50vh;
      }
    </style>
  </head>
  <body onload="initialize()">
    <div class="container">
      <div
        class="contained"
        style="
          background-image: url('/food2.jpg');
          max-width: 100%;
          height: 40%;
          background-repeat: no-repeat;
          justify-content: top;
          position: relative;
          margin: 0 auto;
          clear: left;
          z-index: 0;
          text-align: center;
        "
      >
        <h1 style="color: rgb(238, 196, 117); border: 3px">
          Find Food Near You!
        </h1>
        <div>
          <h3 style="color: rgb(252, 247, 238); border: 3px">
            Enter City or ZipCode
          </h3>
          <input
            id="address"
            type="text"
            style="
              padding: 12px 20px;
              margin: 8px 0;
              box-sizing: border-box;
              border-radius: 4px;
            "
            pattern="[A-Za-z]"
            ;
            autofocus;
            required
          />
        </div>
        <input
          type="button"
          value="Find"
          onclick="codeAddress()"
          style="
            padding: 10px 24px;
            font-size: 14px;
            border-radius: 2px;
            background-color: #92a3c9;
            font-weight: bold;
          "
        />
        <ul id="dynamic-list"></ul>
      </div>
      <div class="contained" id="map" style="width: 100%; height: 60%"></div>
    </div>
  </body>
</html>
